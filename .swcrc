{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "target": "es2022",
    "baseUrl": ".",
    "parser": {
      "syntax": "typescript"
    },
    "paths": {
      "@app": ["src"],
      "@app/*": ["src/*"]
    }
  },
  "module": {
    "noInterop": true,
    "type": "es6",
    "strict": true
  },
  "env": {
    "targets": {
      "node": "18"
    }
  },
  "minify": true
}
